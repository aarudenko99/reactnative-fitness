{
  "expo": {
    "name": "JoesFitness",
    "slug": "JoesFitness",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bug8.JoesFitness"
    },
    "android": {
      "package": "com.bug8.JoesFitness"
    },
    "description": "A gym app built for bodybuilders who want to make a detailed workout plan and record their progress.",
    "githubUrl": "https://github.com/johowoo/FitnessAPP"
  },
  "infoPlist": {
    "NSCameraUsageDescription": "This app uses the camera and album to record progress photos so that users can compare them to see their progress."
  }
}
